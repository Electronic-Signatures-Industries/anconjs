{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "File",
  "type": "object",
  "required": [
    "cid",
    "content",
    "content_type",
    "mode",
    "path",
    "size"
  ],
  "properties": {
    "cid": {
      "type": "string"
    },
    "content": {
      "type": "array",
      "items": {
        "type": "integer",
        "format": "uint8",
        "minimum": 0.0
      }
    },
    "content_type": {
      "type": "string"
    },
    "mode": {
      "type": "string"
    },
    "path": {
      "type": "string"
    },
    "size": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  }
}
