{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MetadataStorage",
  "type": "object",
  "required": [
    "cid",
    "data"
  ],
  "properties": {
    "cid": {
      "type": "string"
    },
    "data": {
      "type": "array",
      "items": {
        "type": "integer",
        "format": "uint8",
        "minimum": 0.0
      }
    }
  }
}
